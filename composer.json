{
    "name": "jezzis/mysqlsyncer",
    "description": "This package provides a easy way to synchronize database structure to laravel",
    "keywords": ["laravel","illuminate","database","synchronize"],
    "license": "MIT",
    "authors": [
        {
            "name": "jezzis",
            "email": "songzijian728@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "cogpowered/finediff": "^0.3.1",
        "illuminate/console": "~5.2",
        "illuminate/database": "~5.2",
        "illuminate/support": "~5.2",
        "illuminate/config": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "dev-master",
        "sami/sami": "~3.3",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "Jezzis\\MysqlSyncer\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
