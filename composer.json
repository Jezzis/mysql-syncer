{
    "name": "jezzis/mysqlsyncer",
    "description": "This package provides a easy way to synchronize database structure to laravel",
    "keywords": ["laravel","illuminate","database","synchronize"],
    "license": "MIT",
    "authors": [
        {
            "name": "jezzis",
            "email": "songzijian727@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/console": "~5.2",
        "illuminate/database": "~5.2",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "dev-master",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "Jezzis\\MysqlSyncer\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
